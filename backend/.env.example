# Orlando Savings Engine - Environment Variables
# Copy this file to .env and fill in your values

# SerpApi Configuration
# Get your API key from: https://serpapi.com/manage-api-key
SERP_API_KEY=your_serpapi_key_here

# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# Optional: Future API integrations
# AMADEUS_API_KEY=your_amadeus_key_here
# AMADEUS_API_SECRET=your_amadeus_secret_here
